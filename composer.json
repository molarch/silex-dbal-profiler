{
    "name": "molarch/silex-dbal-profiler",
    "description": "Doctrine DBAL Profiler for Silex",
    "keywords": [
        "silex",
        "profiler",
        "doctrine",
        "dbal"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Turza",
            "role": "Developer"
        },
        {
            "name": "John Layt",
            "email": "jlayt@mola.org.uk"
        }

    ],
    "replace": {
        "sorien/silex-dbal-profiler": "self.version"
    },
    "require": {
        "molarch/silex": "dev-master",
        "molarch/web-profiler": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Sorien": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/molarch/silex"
        },
        {
            "type": "vcs",
            "url": "https://github.com/molarch/Silex-WebProfiler"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    },
    "require-dev": {
        "rector/rector": "^0.18.1"
    }
}
